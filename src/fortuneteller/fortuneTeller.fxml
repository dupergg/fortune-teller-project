<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="600.0" stylesheets="@../css/defaults.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fortuneteller.FortuneTellerController">
   <children>
      <VBox>
         <children>
            <HBox alignment="CENTER" prefHeight="100.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Fortune Teller" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="72.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
            <VBox prefHeight="600.0" prefWidth="600.0">
               <children>
                  <TextArea fx:id="outputTextArea" editable="false" focusTraversable="false" prefHeight="500.0" text="This is some text." wrapText="true">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </TextArea>
                  <TextArea fx:id="inputTextArea" editable="true" focusTraversable="true" prefHeight="100.0" text="put prompt here!" wrapText="true">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </TextArea>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></VBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPressed" text="Back">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Button>
                  <Button fx:id="testButton" mnemonicParsing="false" onAction="#testButtonPressed" text="Test">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
